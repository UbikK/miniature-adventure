{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "deno.enablePaths": [
    "./packages/application/server",
    "./packages/domain/src",
    "./packages/infra/src"
  ],
  "nuxt.isNuxtApp": false,
  "react-native-tools.projectRoot": "./app",
  "deno.config": "./packages/application/server/deno.json",
  "deno.importMap": "./packages/application/server/import_map.json",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "prettier.enable": false
}
